{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Github indexing schema",
    "type": "object",
    "properties": {
      "docid": {
        "type": "string",
        "description": "+++ docid"
      },
      "stargazers_count": {
        "type": "integer",
        "description": "XXX Repo-level number of stars or upvotes"
      },
      "forks_count": {
        "type": "integer",
        "description": "XXX Repo-level number of forks"
      },
      "size": {
        "type": "integer",
        "description": "+++ File size Repository size (in kB, GitHub only)"
      },
      "html_url": {
        "type": "string",
        "description": "+++ Landing page URL"
      },
      "git_url": {
        "type": "string",
        "description": "Git URL (different from html_url only for GitHub)"
      },
      "source": {
        "type": "string",
        "description": "Repository ('GitHub' or 'Kaggle')"
      },
      "code_file": {
        "type": "string",
        "description": "+++ Notebook filename"
      }
    },
    "required": [
      "docid",
      "stargazers_count",
      "forks_count",
      "size",
      "html_url",
      "git_url",
      "source",
      "code_file"
    ]
  }
  