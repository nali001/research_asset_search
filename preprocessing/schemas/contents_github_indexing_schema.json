{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Github indexing schema",
    "type": "object",
    "properties": {
      "docid": {
        "type": "string",
        "description": "Unique identifier for the notebook"
      },
      "name": {
        "type": "string",
        "description": "??? Short name (GitHub repo name or Kaggle notebook name)"
      },
      "description": {
        "type": "string",
        "description": "??? Short description (Kaggle notebook title or GitHub repository description)"
      },
      "language": {
        "type": "string",
        "description": "??? Programming language"
      }, 
      "query": {
        "type": "string",
        "description": "Query term that returned the notebook"
      }
    },
    "required": [
      "docid",
      "name",
      "description",
      "language",
      "query"
    ]
  }
  