{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Kaggle indexing with summarization schema",
  "type": "object",
  "properties": {
    "docid": {
      "type": "string",
      "description": "??? Unique identifier assigned to the document"
    },
    "source_id": {
      "type": "string",
      "description": "+++ Kaggle id for the notebook"
    },
    "name": {
      "type": "string",
      "description": "+++ Name or title of the document"
    },
    "file_name": {
      "type": "string",
      "description": "+++ File name"
    },
    "html_url": {
      "type": "string",
      "description": "URL of the document in HTML format"
    },
    "source": {
      "type": "string",
      "description": "Name of the organization or entity that created the document"
    }
  },
  "required": [
    "docid", 
    "source_id",
    "name",
    "file_name",
    "html_url",
    "source"
  ]
}
